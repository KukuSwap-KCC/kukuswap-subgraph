{
  "name": "kukuswap-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/kukuswap-kcc/kukuswap-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "create-mainnet": "graph create kukuswap --node https://graph.kukuswap.io/deploy/",
    "deploy-mainnet": "graph deploy kukuswap --ipfs https://graph.kukuswap.io/ipfs/ --node https://graph.kukuswap.io/deploy/ --debug",
    "format:check": "prettier --check '*/**/*.{js,ts}'",
    "format:write": "prettier --write '*/**/*.{js,ts}'"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
